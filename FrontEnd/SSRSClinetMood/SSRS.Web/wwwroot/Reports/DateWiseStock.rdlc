  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>DECLARE @FromDate DATE = '2025-05-01';
DECLARE @ToDate DATE = '2025-05-05';

SELECT 
    p.Id,
    p.Name,

    -- Opening Stock = Current Stock + total sold before FromDate
    (p.StockQty + ISNULL((
        SELECT SUM(s.QuantitySold) 
        FROM Sales s 
        WHERE s.ProductId = p.Id 
          AND CAST(s.SaleDate AS DATE) &lt; @FromDate
    ), 0)) AS OpeningStock,

    -- Total Sold in the given date range (corrected)
    ISNULL((
        SELECT SUM(s.QuantitySold)
        FROM Sales s
        WHERE s.ProductId = p.Id 
          AND CAST(s.SaleDate AS DATE) BETWEEN @FromDate AND @ToDate
    ), 0) AS TotalSoldQuantity,

    -- Closing Stock = Opening Stock - Sold
    ((p.StockQty + ISNULL((
        SELECT SUM(s.QuantitySold)
        FROM Sales s 
        WHERE s.ProductId = p.Id 
          AND CAST(s.SaleDate AS DATE) &lt; @FromDate
    ), 0)) - ISNULL((
        SELECT SUM(s.QuantitySold)
        FROM Sales s
        WHERE s.ProductId = p.Id 
          AND CAST(s.SaleDate AS DATE) BETWEEN @FromDate AND @ToDate
    ), 0)) AS ClosingStock

FROM Products p
WHERE p.IsDeleted = 0;</CommandText>
